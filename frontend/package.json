{
  "name": "top-clip2",
  "version": "1.3.0",
  "description": "A twitch extension showing the streamer's top clip.",
  "license": "MIT",
  "author": "Martin Beierling <mbeierling@gmail.com>",
  "scripts": {
    "start": "concurrently \"parcel watch src/viewer.html src/video_component.html src/config.html src/live_config.html -d dist --public-url ./\" \"yarn dev-server\"",
    "dev-server": "node server.js",
    "build": "parcel build src/viewer.html src/video_component.html src/config.html src/live_config.html -d dist --no-minify --public-url ./",
    "zip": "cross-env bestzip dist_zip/$npm_package_version.zip dist/*",
    "package": "npm run build && npm run zip"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "moment": "^2.20.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hover-observer": "^2.1.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.81.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-eslint": "7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-remove-console": "^6.9.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "bestzip": "^1.1.4",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "eslint": "4.1.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "2.34.1",
    "eslint-plugin-import": "2.6.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "express": "^4.16.2",
    "https": "^1.0.0",
    "parcel-bundler": "^1.8.1",
    "path": "^0.12.7"
  }
}
